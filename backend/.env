# ✅ MongoDB Connection
MONGO_URI=mongodb://127.0.0.1:27017/mindcare  # Ensure MongoDB is running

# ✅ Server Configuration
PORT=5000  # Set the server port

# ✅ JWT Secret Key (For Authentication)
JWT_SECRET= openssl rand -base64 32  # Generate a random secret key

# ✅ Google OAuth Credentials
GOOGLE_CLIENT_ID=your_google_client_id  # Replace with real Client ID
GOOGLE_CLIENT_SECRET=your_google_client_secret  # Replace with real Client Secret

# ✅ Facebook OAuth Credentials
FACEBOOK_CLIENT_ID=your_facebook_client_id  # Replace with real Client ID
FACEBOOK_CLIENT_SECRET=your_facebook_client_secret  # Replace with real Client Secret

